CodeBehindForm
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = True
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Option Compare Database
Option Explicit

Private Sub btnLoeschen_Click()

    'Application.DisplayAlerts = False
    DoCmd.SetWarnings False
    
        If MsgBox("Veranstaltung löschen?" & vbCrLf & _
                  "Soll die Veranstaltung" & Me.txtVeranstaltungName & "  gelöscht werden?", vbYesNo, "Veranstaltung löschen") = vbYes Then
            DoCmd.RunCommand acCmdDeleteRecord
            DoCmd.SetWarnings True
            Form.Refresh
        End If
'
    'Application.DisplayAlerts = False
    

End Sub

Private Sub dateVeranstaltungAnfang_GotFocus()
    Me.dateVeranstaltungAnfang.SetFocus
    Me.dateVeranstaltungAnfang.SelStart = 1
End Sub
